{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "functions": {
      "app.py": {
        "runtime": "python3.9",
        "includeFiles": [
          "models/model_rf.pkl",
          "models/label_encoder.pkl"
        ]
      }
    }
  }
  